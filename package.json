{
  "name": "stitch",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type:check": "tsc",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|ts|tsx|md|gql|graphql|mdx|svelte)\"",
    "format": "pnpm prettier --write",
    "format:check": "pnpm prettier --list-different",
    "validate": "npm-run-all --parallel type:check lint format:check build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "classname": "^0.0.0",
    "classnames": "^2.3.2",
    "eslint": "8.27.0",
    "eslint-config-next": "13.0.3",
    "next": "13.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1"
  }
}
